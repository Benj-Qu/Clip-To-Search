{
  "name": "Clip to Search",
  "description": "A chrome extention that enable users to search by the underlying html codes",
  "version": "1.0",
  "manifest_version": 3,

  "permissions": ["storage"],
  
  "action": {
    "default_popup": "popup.html"
  },

  "content_scripts":[
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ]
  
}